{
    "hud_title_text/title_frame/title": {
        "modifications": [
            {
                "array_name": "bindings",
                "operation": "insert_back",
                "value": [
                    {
                        "binding_type": "view",
                        "source_property_name": "(not (%.7s * #text = 'ui.hud.'))",
                        "target_property_name": "#visible"
                    }
                ]
            }
        ]
    },
    "hud_actionbar_text": {
        "type": "image",
        "size": [
            "100%c + 12px",
            "100%c + 5px"
        ],
        "offset": [
            0,
            "50%-68px"
        ],
        "texture": "textures/ui/hud_tip_text_background",
        "alpha": "@hud.anim_actionbar_text_background_alpha_out",
        "controls": [
            {
                "actionbar_message": {
                    "type": "label",
                    "anchor_from": "center",
                    "anchor_to": "center",
                    "color": "$tool_tip_text",
                    "layer": 31,
                    "text": "$actionbar_text",
                    "localize": false,
                    "alpha": "@hud.anim_actionbar_text_alpha_out"
                }
            }
        ]
    },
    "hud_content": {
        "modifications": [
            {
                "array_name": "controls",
                "operation": "insert_front",
                "value": [
                    {
                        "data_control": {
                            "type": "panel",
                            "property_bag": {
                                "#title_text": ""
                            },
                            "bindings": [
                                {
                                    "binding_name": "#hud_title_text_string"
                                },
                                {
                                    "binding_name": "#hud_title_text_string",
                                    "binding_name_override": "#title_text",
                                    "binding_condition": "visibility_changed"
                                },
                                {
                                    "binding_type": "view",
                                    "source_property_name": "((not (#hud_title_text_string = #title_text)) and ((%.19s * #title_text) = 'ui.hud.det.progress')))",
                                    "target_property_name": "#visible"
                                }
                            ]
                        }
                    }
                ]
            }
        ]
    },
