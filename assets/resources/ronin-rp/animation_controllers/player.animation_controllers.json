{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.player.root": {
			"initial_state": "third_person",
			"states": {
				"first_person": {
					"animations": [
						{"first_person_swap_item": "!query.blocking"},
						{"first_person_shield_block": "query.blocking"},
						{"first_person_attack_controller": "variable.attack_time > 0.0f && query.get_equipped_item_name != 'filled_map'"},
						"first_person_base_pose",
						{"first_person_empty_hand": "query.get_equipped_item_name(0, 1) != 'filled_map'"},
						{"first_person_walk": "variable.bob_animation"},
						{"first_person_map_controller": "(query.get_equipped_item_name(0, 1) == 'filled_map' || query.get_equipped_item_name('off_hand') == 'filled_map')"},
						{"first_person_crossbow_equipped": "query.get_equipped_item_name == 'crossbow' && (variable.item_use_normalized > 0 && variable.item_use_normalized < 1.0)"},
						{"first_person_breathing_bob": "variable.attack_time <= 0.0"}
					],
					"transitions": [
						{"paperdoll": "variable.is_paperdoll"},
						{"map_player": "variable.map_face_icon"},
						{"third_person": "!variable.is_first_person"}
					]
				},
				"map_player": {
					"transitions": [
						{"paperdoll": "variable.is_paperdoll"},
						{"first_person": "variable.is_first_person"},
						{"third_person": "!variable.map_face_icon && !variable.is_first_person"}
					]
				},
				"paperdoll": {
					"animations": ["humanoid_base_pose", "look_at_target_ui", "cape"],
					"transitions": [
						{"first_person": "!variable.is_paperdoll && variable.is_first_person"},
						{"map_player": "variable.map_face_icon"},
						{"third_person": "!variable.is_paperdoll && !variable.is_first_person"}
					]
				},
				"third_person": {
					"animations": [
						{"look_at_target": "!query.is_sleeping"},
						"controller.move",
						"cape"
					],
					"transitions": [
						{"paperdoll": "variable.is_paperdoll"},
						{"first_person": "variable.is_first_person"},
						{"map_player": "variable.map_face_icon"}
					]
				}
			}
		},
		"controller.animation.player.move": {
			"initial_state": "idle",
			"states": {
				"idle": {
                    "animations": [
                        "custom.stand"
                    ],
					"transitions": [
						{"walkL": "q.modified_move_speed > 0.1 && v.leg_front == 1"},
						{"walkR": "q.modified_move_speed > 0.1 && v.leg_front == 2"},
						{"jumpL": "q.is_jumping && v.leg_front == 1 && q.is_moving"},
						{"jumpR": "q.is_jumping && v.leg_front == 2 && q.is_moving"},
                        {"jumpInSpace": "q.is_jumping && !q.is_moving"},
						{"falling": "!q.is_on_ground"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
				"walkL": {
					"animations": [
						{"custom.walk": "q.is_on_ground"}
					],
					"transitions": [
						{"idle": "q.modified_move_speed < 0.1"},
						{"runningL": "q.is_sprinting && v.leg_front == 1"},
						{"runningR": "q.is_sprinting && v.leg_front == 2"},
						{"jumpL": "q.is_jumping && v.leg_front == 1"},
						{"jumpR": "q.is_jumping && v.leg_front == 2"},
						{"falling": "!q.is_on_ground"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
                "walkR": {
					"animations": [
						{"custom.walk2": "q.is_on_ground"}
					],
					"transitions": [
						{"idle": "q.modified_move_speed < 0.1"},
						{"runningL": "q.is_sprinting && v.leg_front == 1"},
						{"runningR": "q.is_sprinting && v.leg_front == 2"},
						{"jumpL": "q.is_jumping && v.leg_front == 1"},
						{"jumpR": "q.is_jumping && v.leg_front == 2"},
						{"falling": "!q.is_on_ground"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
				"runningL": {
					"animations": [
						{"holding": "q.is_on_ground"}
					],
					"transitions": [
                        {"walkL": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 1"},
						{"walkR": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 2"},
						{"jumpL": "q.is_jumping && v.leg_front == 1"},
						{"jumpR": "q.is_jumping && v.leg_front == 2"},
						{"falling": "!q.is_on_ground"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
                "runningR": {
					"animations": [
						{"holding2": "q.is_on_ground"}
					],
					"transitions": [
						{"walkL": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 1"},
						{"walkR": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 2"},
						{"jumpL": "q.is_jumping && v.leg_front == 1"},
						{"jumpR": "q.is_jumping && v.leg_front == 2"},
						{"falling": "!q.is_on_ground"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
				"jumpR": {
					"animations": [
						{"jump.right": "!q.is_gliding"}
					],
					"transitions": [
						{"landingL": "q.is_on_ground"},
						{"falling": "q.vertical_speed < -8"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
                "jumpL": {
					"animations": [
						{"jump.left": "!q.is_gliding"}
					],
					"transitions": [
                        {"landingR": "q.is_on_ground"},
						{"falling": "q.vertical_speed < -8"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
                "jumpInSpace": {
					"animations": [
						{"jump.in_space": "!q.is_gliding"}
					],
					"transitions": [
                        {"landingInSpace": "q.is_on_ground"},
						{"falling": "q.vertical_speed < -8"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
                "landingL": {
                    "animations": [
						"landing.left"
					],
					"transitions": [
						{"walkL": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 1"},
						{"walkR": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 2"},
                        {"runningL": "q.is_sprinting && v.leg_front == 1"},
						{"runningR": "q.is_sprinting && v.leg_front == 2"},
                        {"idle": "!q.is_moving"}
					],
					"blend_transition": 0.3,
					"blend_via_shortest_path": true
                },
                "landingR": {
                    "animations": [
						"landing.right"
					],
					"transitions": [
						{"walkL": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 1"},
						{"walkR": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 2"},
                        {"runningL": "q.is_sprinting && v.leg_front == 1"},
						{"runningR": "q.is_sprinting && v.leg_front == 2"},
                        {"idle": "!q.is_moving"}
					],
					"blend_transition": 0.3,
					"blend_via_shortest_path": true
                },
                "landingInSpace": {
                    "animations": [
						"landing.in_space"
					],
					"transitions": [
						{"walkL": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 1"},
						{"walkR": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 2"},
                        {"runningL": "q.is_sprinting && v.leg_front == 1"},
						{"runningR": "q.is_sprinting && v.leg_front == 2"},
                        {"idle": "!q.is_moving"}
					],
					"blend_transition":0.3,
					"blend_via_shortest_path": true
                },
				"falling": {
					"animations": ["custom.fall"],
					"transitions": [
                        {"runningL": "q.is_on_ground && q.is_sprinting && v.leg_front == 1"},
						{"runningR": "q.is_on_ground && q.is_sprinting && v.leg_front == 2"},
						{"walkL": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 1"},
						{"walkR": "!q.is_sprinting && q.modified_move_speed > 0.1 && v.leg_front == 2"},
                        {"idle": "q.is_on_ground && q.modified_move_speed < 0.1 && !q.is_moving"}
					],
					"blend_transition": 0.3,
					"blend_via_shortest_path": true
				}
			}
		}
	}
}