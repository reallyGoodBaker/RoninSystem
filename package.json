{
    "name": "ronin-system",
    "version": "1.0.0",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "link": "node ./scripts/link.js",
        "prebuild": "rollup -c tool.config.js && node ./dist/tools/generator.cjs",
        "build": "npm run prebuild && rollup -c rollup.config.js",
        "watch": "npm run prebuild && rollup -c -w",
        "build-editor": "cd ./src/editor && rollup -c && npx @tailwindcss/cli -i ./style.css -o ../../dist/editor/style.css",
        "editor": "npm run build-editor && node ./dist/editor/server.cjs",
        "build-addon": "npm run build && node ./dist/tools/bundleFiles.cjs"
    },
    "author": "RGB39",
    "license": "MIT",
    "description": "An advanced combat system for Minecraft Bedrock Edition.",
    "devDependencies": {
        "@minecraft/server": "^2.4.0-beta.1.21.123-stable",
        "@minecraft/server-ui": "^2.0.0",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.4",
        "@types/sockjs": "^0.3.36",
        "blockbench-types": "^5.0.0",
        "rollup": "^4.52.5",
        "rollup-plugin-typescript-paths": "^1.5.0",
        "tailwindcss": "^4.1.16",
        "ts-node": "^10.9.2",
        "tsconfig-replace-paths": "^0.0.14",
        "tslib": "^2.8.1",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "@minecraft/math": "^2.2.11",
        "@rollup/plugin-commonjs": "^28.0.6",
        "@tailwindcss/cli": "^4.1.16",
        "@types/adm-zip": "^0.5.7",
        "@types/ws": "^8.18.1",
        "adm-zip": "^0.5.16",
        "codemirror": "^6.0.2",
        "level": "^10.0.0",
        "material-symbols": "^0.40.0",
        "monaco-editor": "^0.55.1",
        "reflect-metadata": "^0.2.2",
        "sockjs": "^0.3.24",
        "typedi": "^0.10.0",
        "ws": "^8.18.3"
    }
}
